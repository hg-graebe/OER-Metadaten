able identifier="lambda1" /><imsqti:variable identifier="a" /><imsqti:variable identifier="lambda2" /></imsqti:customOperator></imsqti:setTemplateValue><imsqti:setCorrectResponse identifier="RESPONSE_1403599272737"><imsqti:customOperator definition="VARIABLESTRING" value="$(2)/$(1)"><imsqti:variable identifier="a" /><imsqti:variable identifier="b" /></imsqti:customOperator></imsqti:setCorrectResponse><imsqti:setCorrectResponse identifier="RESPONSE_1403599311379"><imsqti:customOperator definition="VARIABLESTRING" value="-$(1)/$(2)"><imsqti:variable identifier="a" /><imsqti:variable identifier="b" /></imsqti:customOperator></imsqti:setCorrectResponse><imsqti:setCorrectResponse identifier="RESPONSE_1403599742800"><imsqti:variable identifier="lambda1" /></imsqti:setCorrectResponse><imsqti:setCorrectResponse identifier="RESPONSE_1403599756138"><imsqti:variable identifier="lambda2" /></imsqti:setCorrectResponse></imsqti:templateProcessing><itemBody ><p><span style="font-size: 10pt;">Wir betrachten das lineare Differentialgleichungssystem</span></p><p><span style="font-size: 10pt;">\(\begin{pmatrix}y_1'(x)\\y_2'(x)\end{pmatrix}=A\cdot\begin{pmatrix}y_1(x)\\y_2(x)\end{pmatrix}\), wobei \(\displaystyle A=\frac{1}{<printedVariable identifier="sum_quad" />}\begin{pmatrix}<printedVariable identifier="a1" />&amp;<printedVariable identifier="a23" />\\<printedVariable identifier="a23" />&amp;<printedVariable identifier="a4" />\end{pmatrix}\).</span></p><p><span style="font-size: 10pt;">Die allgemeine Lösung kann in der Form</span></p><p><span style="font-size: 10pt;">\(\displaystyle\begin{pmatrix}y_1(x)\\y_2(x)\end{pmatrix}=c_1\begin{pmatrix}1\\v_1\end{pmatrix} \mathrm e^{\lambda_1 x} + c_2\begin{pmatrix}1\\v_2\end{pmatrix} \mathrm e^{\lambda_2 x}\)</span></p><p><span style="font-size: 10pt;">geschrieben werden. Geben Sie \(\lambda_1,\lambda_2,v_1\) sowie \(v_2\) an! </span></p><p><strong><span style="font-size: 10pt;">Nummerieren Sie \(\lambda_1\), \(\lambda_2\) nach aufsteigender Größe, d.h. es soll \(\lambda_1\leq\lambda_2\) sein!</span></strong></p><p><span style="font-size: 10pt;">\(\lambda_1\) = <textEntryInteraction responseIdentifier="RESPONSE_1403599742800" expectedLength="2" /> , \(v_1=\) <textEntryInteraction responseIdentifier="RESPONSE_1403599272737" expectedLength="4" class="maxima-formula" /> </span></p><p><span style="font-size: 10pt;">\(\lambda_2\) = <textEntryInteraction responseIdentifier="RESPONSE_1403599756138" expectedLength="2" /> , \(v_2=\) <textEntryInteraction responseIdentifier="RESPONSE_1403599311379" expectedLength="4" class="maxima-formula" /> </span></p><p><span style="font-size: 10pt;"><strong>Hinweis:</strong> Sie können Brüche wiefolgt eingeben: \(\frac ab=\) <strong>a/b</strong></span></p></itemBody><imsqti:responseProcessing xmlns:imsqti="http://www.imsglobal.org/xsd/imsqti_v2p1"><imsqti:responseCondition><imsqti:responseIf><imsqti:equal toleranceMode="exact"><imsqti:variable identifier="RESPONSE_1403599742800" /><imsqti:correct identifier="RESPONSE_1403599742800" /></imsqti:equal><imsqti:setOutcomeValue identifier="SCORE"><imsqti:sum><imsqti:variable identifier="SCORE" /><imsqti:baseValue baseType="float">1</imsqti:baseValue></imsqti:sum></imsqti:setOutcomeValue></imsqti:responseIf></imsqti:responseCondition><imsqti:responseCondition><imsqti:responseIf><imsqti:not><imsqti:isNull><imsqti:variable identifier="RESPONSE_1403599742800" /></imsqti:isNull></imsqti:not><imsqti:setOutcomeValue identifier="FEEDBACKBASIC"><imsqti:baseValue baseType="identifier">incorrect</imsqti:baseValue></imsqti:setOutcomeValue></imsqti:responseIf></imsqti:responseCondition><imsqti:responseCondition><imsqti:responseIf><imsqti:customOperator definition="MAXIMA" value="is(equal(ev($(1)),ev($(2))));" caseSensitive="false"><imsqti:variable identifier="RESPONSE_1403599272737" /><imsqti:correct identifier="RESPONSE_1403599272737" /></imsqti:customOperator><imsqti:setOutcomeValue identifier="SCORE"><imsqti:sum><imsqti:variable identifier